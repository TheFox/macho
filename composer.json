{
	"name": "thefox/macho",
	"description": "PHP script for modifying Mach-O executable i386 and x86_64 files.",
	"license": "GPL-3.0",
	"type": "project",
	"keywords": [ "Mach", "MachO", "Binary", "x86", "i386", "x86_64", "x86-64" ],
	"homepage": "http://fox21.at",
	"authors": [
		{
			"name": "Christian Mayer",
			"email": "christian@fox21.at",
			"homepage": "http://fox21.at"
		}
	],
	"require": {
		"php": ">=5.3",
		"symfony/filesystem": "~2.5",
		"symfony/finder": "~2.5",
		"liip/process-manager": "~1.0",
		"thefox/utilities": "~1.2",
		"thefox/assembly": "~0.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.1",
		"doctrine/common": "~2.4",
		"satooshi/php-coveralls": "dev-master",
		"thefox/phpcsrs": "~1.0"
	},
	"autoload": {
		"psr-0": { "TheFox": "src" }
	},
	"autoload-dev": {
		"psr-0": { "TheFox": "tests" }
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x.x-dev"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"echo post-install-cmd",
			"chmod a+x application.php",
			"php bootstrap.php"
		],
		"post-update-cmd": [
			"echo post-update-cmd",
			"php bootstrap.php"
		]
	}
}
